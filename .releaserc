{
  "plugins": [
    ["@semantic-release/git", {
      "assets": ["dist/**/*.{js,css}", "docs", "package.json"],
      "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
    }]
  ],
  "branches": [
    "+([0-9])?(.{+([0-9]),x}).x",
    "main",
    "next",
    "next-major",
    {
      "name": "beta",
      "channel": "beta",
      "prerelease": true
    },
    {
      "name": "alpha",
      "channel": "alpha",
      "prerelease": true
    }
  ]
}